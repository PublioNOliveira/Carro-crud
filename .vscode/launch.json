{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Carro-Carro",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsCarro/test/flpSandbox.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Carro",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Carro/Carro\"}"
      },
      "envFile": "${workspaceFolder}/Carro/.env1",
      "preLaunchTask": "Build Carro"
    }
  ]
}